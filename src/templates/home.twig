{% extends "base.twig" %}
{% block title %}Home{% endblock %}
{% block content %}
<div class="container">
  <div id="carousel-home" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
      <div class="item active"><img src="/carousel01.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel02.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel03.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel04.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel05.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel06.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel07.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel08.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel09.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel10.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel11.jpg" class="center-block"></div>
      <div class="item"><img src="/carousel12.jpg" class="center-block"></div>
    </div>

    <a class="left carousel-control" href="#carousel-home" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
    </a>
    <a class="right carousel-control" href="#carousel-home" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
    </a>
  </div>
</div>
{% endblock %}
